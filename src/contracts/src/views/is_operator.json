                                {
                                    "prim": "PUSH",
                                    "args": [
                                        {
                                            "prim": "bool"
                                        },
                                        {
                                            "nat": "False"
                                        }
                                    ]
                                }
                            ],
                            [
                                {
                                    "prim": "PUSH",
                                    "args": [
                                        {
                                            "prim": "bool"
                                        },
                                        {
                                            "nat": "True"
                                        }
                                    ]
                                }
                            ]
                        ]
                    }
                ]
                }
            }]
